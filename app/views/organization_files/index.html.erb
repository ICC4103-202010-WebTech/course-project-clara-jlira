<p id="notice"><%= notice %></p>

<h1>Organization Files</h1>

<table width="30%">
  <thead>
    <tr>
      <th>file_dir</th>
      <th>file_type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @organization_files.each do |organization_file| %>
      <tr>
        <td><%= organization_file.file_dir %></td>
        <td><%= organization_file.file_type %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= link_to 'Back', :back %>
