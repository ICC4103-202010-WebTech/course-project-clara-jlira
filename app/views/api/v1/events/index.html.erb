<p id="notice"><%= notice %></p>

<h1>Api V1 Events</h1>

<table>
  <thead>
    <tr>
      <th>Index</th>
      <th>Show</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_events.each do |api_v1_event| %>
      <tr>
        <td><%= api_v1_event.index %></td>
        <td><%= api_v1_event.show %></td>
        <td><%= link_to 'Show', api_v1_event %></td>
        <td><%= link_to 'Edit', edit_api_v1_event_path(api_v1_event) %></td>
        <td><%= link_to 'Destroy', api_v1_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Event', new_api_v1_event_path %>
