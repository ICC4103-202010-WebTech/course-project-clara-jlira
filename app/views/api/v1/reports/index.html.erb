<p id="notice"><%= notice %></p>

<h1>Api V1 Reports</h1>

<table>
  <thead>
    <tr>
      <th>Index</th>
      <th>Show</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_reports.each do |api_v1_report| %>
      <tr>
        <td><%= api_v1_report.index %></td>
        <td><%= api_v1_report.show %></td>
        <td><%= link_to 'Show', api_v1_report %></td>
        <td><%= link_to 'Edit', edit_api_v1_report_path(api_v1_report) %></td>
        <td><%= link_to 'Destroy', api_v1_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Report', new_api_v1_report_path %>
