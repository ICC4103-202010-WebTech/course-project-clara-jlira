<p id="notice"><%= notice %></p>

<h1>Api V1 Invitations</h1>

<table>
  <thead>
    <tr>
      <th>Index</th>
      <th>Show</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_invitations.each do |api_v1_invitation| %>
      <tr>
        <td><%= api_v1_invitation.index %></td>
        <td><%= api_v1_invitation.show %></td>
        <td><%= link_to 'Show', api_v1_invitation %></td>
        <td><%= link_to 'Edit', edit_api_v1_invitation_path(api_v1_invitation) %></td>
        <td><%= link_to 'Destroy', api_v1_invitation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Invitation', new_api_v1_invitation_path %>
