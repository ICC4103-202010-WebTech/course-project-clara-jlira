<p id="notice"><%= notice %></p>

<h1>Api V1 Memberships</h1>

<table>
  <thead>
    <tr>
      <th>Index</th>
      <th>Show</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_memberships.each do |api_v1_membership| %>
      <tr>
        <td><%= api_v1_membership.index %></td>
        <td><%= api_v1_membership.show %></td>
        <td><%= link_to 'Show', api_v1_membership %></td>
        <td><%= link_to 'Edit', edit_api_v1_membership_path(api_v1_membership) %></td>
        <td><%= link_to 'Destroy', api_v1_membership, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Membership', new_api_v1_membership_path %>
