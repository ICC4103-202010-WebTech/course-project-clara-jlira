<p id="notice"><%= notice %></p>

<h1>Api V1 Organizations</h1>

<table>
  <thead>
    <tr>
      <th>Index</th>
      <th>Show</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_organizations.each do |api_v1_organization| %>
      <tr>
        <td><%= api_v1_organization.index %></td>
        <td><%= api_v1_organization.show %></td>
        <td><%= link_to 'Show', api_v1_organization %></td>
        <td><%= link_to 'Edit', edit_api_v1_organization_path(api_v1_organization) %></td>
        <td><%= link_to 'Destroy', api_v1_organization, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Organization', new_api_v1_organization_path %>
