<p id="notice"><%= notice %></p>

<h1>Api V1 Notifications</h1>

<table>
  <thead>
    <tr>
      <th>Index</th>
      <th>Show</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_notifications.each do |api_v1_notification| %>
      <tr>
        <td><%= api_v1_notification.index %></td>
        <td><%= api_v1_notification.show %></td>
        <td><%= link_to 'Show', api_v1_notification %></td>
        <td><%= link_to 'Edit', edit_api_v1_notification_path(api_v1_notification) %></td>
        <td><%= link_to 'Destroy', api_v1_notification, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Notification', new_api_v1_notification_path %>
