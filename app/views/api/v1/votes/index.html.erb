<p id="notice"><%= notice %></p>

<h1>Api V1 Votes</h1>

<table>
  <thead>
    <tr>
      <th>Index</th>
      <th>Show</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_votes.each do |api_v1_vote| %>
      <tr>
        <td><%= api_v1_vote.index %></td>
        <td><%= api_v1_vote.show %></td>
        <td><%= link_to 'Show', api_v1_vote %></td>
        <td><%= link_to 'Edit', edit_api_v1_vote_path(api_v1_vote) %></td>
        <td><%= link_to 'Destroy', api_v1_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Vote', new_api_v1_vote_path %>
