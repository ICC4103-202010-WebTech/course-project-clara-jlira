<p id="notice"><%= notice %></p>

<h1>Api V1 Replies</h1>

<table>
  <thead>
    <tr>
      <th>Index</th>
      <th>Show</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_replies.each do |api_v1_reply| %>
      <tr>
        <td><%= api_v1_reply.index %></td>
        <td><%= api_v1_reply.show %></td>
        <td><%= link_to 'Show', api_v1_reply %></td>
        <td><%= link_to 'Edit', edit_api_v1_reply_path(api_v1_reply) %></td>
        <td><%= link_to 'Destroy', api_v1_reply, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Reply', new_api_v1_reply_path %>
