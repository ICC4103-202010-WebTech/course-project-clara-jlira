<p id="notice"><%= notice %></p>

<h1>Api V1 Messages</h1>

<table>
  <thead>
    <tr>
      <th>Index</th>
      <th>Show</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_v1_messages.each do |api_v1_message| %>
      <tr>
        <td><%= api_v1_message.index %></td>
        <td><%= api_v1_message.show %></td>
        <td><%= link_to 'Show', api_v1_message %></td>
        <td><%= link_to 'Edit', edit_api_v1_message_path(api_v1_message) %></td>
        <td><%= link_to 'Destroy', api_v1_message, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api V1 Message', new_api_v1_message_path %>
