<p id="notice"><%= notice %></p>

<h1>Event Files</h1>

<table>
  <thead>
    <tr>
      <th>Index</th>
      <th>Show</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @event_files.each do |event_file| %>
      <tr>
        <td><%= event_file.index %></td>
        <td><%= event_file.show %></td>
        <td><%= link_to 'Show', event_file %></td>
        <td><%= link_to 'Edit', edit_event_file_path(event_file) %></td>
        <td><%= link_to 'Destroy', event_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event File', new_event_file_path %>
